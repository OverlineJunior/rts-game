{
	"name": "rts-game",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"server": {
				"$className": "Folder",
				"game": {
					"$path": "out/game/server"
				},
				"squad": {
					"$path": "out/squad/server"
				},
				"unit": {
					"$path": "out/unit/server",
					"replication": {
						"$path": "out/unit/replication/server"
					},
					"selection": {
						"$path": "out/unit/selection/server"
					}
				}
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					}
				}
			},
			"client": {
				"$className": "Folder",
				"game": {
					"$path": "out/game/client"
				},
				"squad": {
					"$path": "out/squad/client"
				},
				"unit": {
					"$path": "out/unit/client",
					"replication": {
						"$path": "out/unit/replication/client"
					},
					"selection": {
						"$path": "out/unit/selection/client"
					}
				}
			},
			"shared": {
				"$className": "Folder",
				"game": {
					"$path": "out/game/shared"
				},
				"squad": {
					"$path": "out/squad/shared"
				},
				"unit": {
					"$path": "out/unit/shared",
					"replication": {
						"$path": "out/unit/replication/shared"
					},
					"selection": {
						"$path": "out/unit/selection/shared"
					}
				}
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"main": {
					"$path": "out/game/client/main.client.luau"
				}
			}
		}
	}
}